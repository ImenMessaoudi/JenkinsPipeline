    agent any

    stages {
        stage('BUILD') {
            steps {
                echo 'Build devlopement'
            }
        }
        stage('TESTING') {
            steps {
                echo 'Hello Testing '
            }
        }
        stage('DEPLOY') {
            steps {
                echo 'Mise  en  production'
            }
        }
    }
}
